<Window x:Class="MDRDeskInstaller.FileUsage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MDRDeskInstaller"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="FileUsage" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0"></TextBlock>
        <ListBox Grid.Row="1" Name="LockedFileList"></ListBox>
        <StackPanel Grid.Row="2" FlowDirection="LeftToRight" Orientation="Horizontal">
            <Button Name="CancelBtn" Click="CancelBtnClicked" Margin="4,2,4,2" FontFamily="Consolas" FontWeight="Bold">_Cancel</Button>
            <Button Name="TryAgainBtn" Click="TryAgainBtnClicked" Margin="4,2,4,2" FontFamily="Consolas" FontWeight="Bold">_Try Again</Button>
        </StackPanel>
        
    </Grid>
</Window>
