<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1"/>
    </startup>
  <appSettings>
    <add key="dacfolder" value=""/>
    <add key="mapfolder" value=""/>
    <add key="recentindices" value=""/>
    <add key="recentadhocs" value=""/>
    <add key="procdumpfolder" value=""/>
    <add key="helpfolder" value=""/>
    <add key="refbuilder" value="c++"/>
    <add key="refreader" value="map"/>
    <!--<add key="wnddbgfolder" value="C:\Program Files (x86)\Windows Kits\10\Debuggers\x64"/>-->
    <!-- type display modes: namespaces, types, fulltypenames-->
    <add key="typedisplaymode" value="namespaces"/>
    <!--<add key="skipreferences" value="false"/>-->
    <add key="skipinstancereferences" value="false"/>
    <add key="skipdeadstackobjects" value="false"/>
  </appSettings>
  <runtime>
    <gcAllowVeryLargeObjects enabled="true"/>
    <ThrowUnobservedTaskExceptions enabled="true"/>
  </runtime>
</configuration>
